{
  "name": "Copycat",
  "identifier": "com.azhsetiawan.sketchplugins.copycat",
  "compatibleVersion": "58",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Create Copycat in Artboard",
      "identifier": "copycat.create",
      "script": "./copycat-create.js",
      "icon": "runner-icon-create.png",
      "iconDark": "runner-icon-create-dark.png",
      "description": "Create hidden #copycat layer in selected Artboard"
    },
    {
      "name": "Update Copycat in Artboard",
      "identifier": "copycat.update",
      "script": "./copycat-update.js",
      "icon": "runner-icon-update.png",
      "iconDark": "runner-icon-update-dark.png",
      "description": "Update hidden #copycat layer in selected Artboard"
    },
    {
      "name": "Settings",
      "identifier": "copycat.settings",
      "script": "./settings.js"
    },
    {
      "name" : "Report Issue",
      "identifier" : "copycat.issue",
      "script": "./menu.js",
      "handler" : "report"
    },
    {
      "name" : "Donate",
			"identifier" : "copycat.donate",
			"script" : "./menu.js",
			"handler" : "donate"
    }    
  ],
  "menu": {
    "title": "Copycat",
    "items": [
      "copycat.create",
      "copycat.update",
      "-",
      "copycat.settings",
      "copycat.issue",
      "copycat.donate"
    ]
  }
}